> set Compile / jooqCodegenAutoStrategy := AutoStrategy.IfAbsent
> compile
> compile
> checkCalledOnce
> clean

> set Compile / jooqCodegenAutoStrategy := AutoStrategy.Always
> compile
> compile
> checkCalledTwice
> clean

> set Compile / jooqCodegenAutoStrategy := AutoStrategy.Never
-> compile
> checkCalledNever
> jooqCodegen
> checkCalledOnce
> compile
> compile
> checkCalledNever
