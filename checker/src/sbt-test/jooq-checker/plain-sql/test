> set jooqCheckerLevelPlainSQL := CheckerLevel.Disabled
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelPlainSQL := CheckerLevel.Warning
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelPlainSQL := CheckerLevel.Error
> show Compile / compile / scalacOptions
-> compile

$ copy-file changes/AllowPlainSQL.scala src/main/scala/com/example/PlainSQLWartTest.scala
> compile
