> set jooqCheckerLevelPlainSQL := JooqCheckerLevel.Disabled
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelPlainSQL := JooqCheckerLevel.Warning
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelPlainSQL := JooqCheckerLevel.Error
> show Compile / compile / scalacOptions
-> compile

$ copy-file changes/AllowPlainSQL.scala src/main/scala/com/example/PlainSQLWartTest.scala
> compile
