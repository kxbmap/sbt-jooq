> set jooqCheckerLevelSQLDialect := CheckerLevel.Disabled
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelSQLDialect := CheckerLevel.Warning
> show Compile / compile / scalacOptions
> compile

> set jooqCheckerLevelSQLDialect := CheckerLevel.Error
> show Compile / compile / scalacOptions
-> compile

$ copy-file changes/AllowAllDialects.scala src/main/scala/com/example/SQLDialectWartTest.scala
> compile
